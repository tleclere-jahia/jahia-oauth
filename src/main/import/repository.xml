<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <jahia-oauth j:dependencies="default"
                   j:modulePriority="0"
                   j:moduleType="module"
                   j:title="Jahia OAuth"
                   jcr:primaryType="jnt:module">

                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <oauth-result j:hiddenTemplate="true"
                          j:view="oauthResult"
                          jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <authentication-result jcr:primaryType="joant:oauthResult"/>
                        </pagecontent>
                    </oauth-result>
                    <google-oauth-view j:defaultTemplate="false"
                                       j:hiddenTemplate="true"
                                       j:invertCondition="false"
                                       j:requireLoggedUser="false"
                                       j:requirePrivilegedUser="false"
                                       jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <googleoauthview jcr:primaryType="joant:googleOAuthView"/>
                        </pagecontent>
                    </google-oauth-view>
                    <franceconnect-oauth-view j:defaultTemplate="false"
                                              j:hiddenTemplate="true"
                                              j:invertCondition="false"
                                              j:requireLoggedUser="false"
                                              j:requirePrivilegedUser="false"
                                              jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <franceconnectoauthview jcr:primaryType="joant:franceConnectOAuthView"/>
                        </pagecontent>
                    </franceconnect-oauth-view>
                    <linkedIn-oauth-view j:defaultTemplate="false"
                                         j:hiddenTemplate="true"
                                         j:invertCondition="false"
                                         j:requireLoggedUser="false"
                                         j:requirePrivilegedUser="false"
                                         jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <linkedinoauthview jcr:primaryType="joant:linkedInOAuthView"/>
                        </pagecontent>
                    </linkedIn-oauth-view>
                </templates>

                <oauth-result j:templateName="oauth-result" jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Authentication Result"/>
                    <j:translation_de jcr:language="de"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Authentication Result"/>
                    <j:translation_pt jcr:language="pt"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Authentication Result"/>
                    <j:translation_it jcr:language="it"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Authentication Result"/>
                    <j:translation_fr jcr:language="fr"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="RÃ©sultat de l'authentification"/>
                    <j:translation_es jcr:language="es"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Authentication Result"/>
                </oauth-result>
            </jahia-oauth>
    </modules>
</content>
